<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="LightBlack">#2e2e2e</Color>
    <Color x:Key="DarkBlack">#212121</Color>
    <Color x:Key="DarkDevBlue">#037ffc</Color>
    <Color x:Key="DevBlue">#FF569DE5</Color>
    <Color x:Key="DevOrange">#CA5100</Color>
    <Color x:Key="WarmWhite">#CFCFCF</Color>
    <Color x:Key="NewDarkGray">#2D2D2E</Color>

    <SolidColorBrush x:Key="LightBlackBrush" Color="{StaticResource LightBlackColor}"/>
    <SolidColorBrush x:Key="DarkBlackBrush" Color="{StaticResource DarkBlackColor}"/>
    <SolidColorBrush x:Key="DevBlueBrush" Color="{StaticResource DevBlueColor}"/>
    <SolidColorBrush x:Key="DarkDevBlueBrush" Color="{StaticResource DarkDevBlueColor}"/>
    <SolidColorBrush x:Key="DevOrangeBrush" Color="{StaticResource DevOrangeColor}"/>
    <SolidColorBrush x:Key="PageBackgroundBrush" Color="{StaticResource NewDarkGray}"/>

</ResourceDictionary>